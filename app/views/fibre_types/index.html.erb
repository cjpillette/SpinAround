<p id="notice"><%= notice %></p>

<h1>Fibre Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Isanimal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fibre_types.each do |fibre_type| %>
      <tr>
        <td><%= fibre_type.name %></td>
        <td><%= fibre_type.isAnimal %></td>
        <td><%= link_to 'Show', fibre_type %></td>
        <td><%= link_to 'Edit', edit_fibre_type_path(fibre_type) %></td>
        <td><%= link_to 'Destroy', fibre_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fibre Type', new_fibre_type_path %>
